{
  "name": "server",
  "version": "1.0.0",
  "description": "Starter template for a Node.js + TypeScript + Express + Jest + MSW server",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aquil3sVoy/server-base"
  },
  "author": "Oscar Corona <oabc4004@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Aquil3sVoy/server-base/issues"
  },
  "keywords": [
    "node",
    "typescript",
    "express",
    "jest",
    "msw"
  ],
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "nodemon --watch \"src/**/*.ts\" -e ts,json --exec \"ts-node --transpile-only ./src/server.ts\"",
    "dev": "ts-node inventory.ts",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-rate-limit": "^6.5.1",
    "helmet": "^5.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^17.0.24",
    "jest": "^28.1.3",
    "msw": "^0.44.2",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
